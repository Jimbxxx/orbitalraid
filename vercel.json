{
  "version": 2,
  "builds": [
    {
      "src": "orbitalraid/api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "orbitalraid/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "orbitalraid/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "orbitalraid/public/$1"
    }
  ]
}
